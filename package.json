{
	"name": "reglace",
	"publisher": "LiXiaYu",
	"description": "replace by regex",
	"license": "MIT",
	"version": "0.0.3",
	"categories": [],
	"repository": {
		"type": "git",
		"url": "https://github.com/LiXiaYu/reglace_vsc.git"
	},
	"engines": {
		"vscode": "^1.43.0"
	},
	"activationEvents": [
		"onLanguage:cpp"
	],
	"contributes": {
		"languages": [
			{
				"id": "cpp",
				"extensions": [
					".mate"
				]
			},
			{
				"id": "json",
				"extensions": [
					".reglace"
				]
			},
			{
				"id": "regmake",
				"extensions": [
					".regmake"
				]
			}
		]
	},
	"main": "./client/out/extension",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -b",
		"watch": "tsc -b -w",
		"postinstall": "cd client && npm install && cd ../server && npm install && cd .."
	},
	"devDependencies": {
		"@types/mocha": "^8.2.2",
		"@types/node": "^12.12.0",
		"@typescript-eslint/eslint-plugin": "^4.23.0",
		"@typescript-eslint/parser": "^4.23.0",
		"eslint": "^7.26.0",
		"mocha": "^8.3.2",
		"typescript": "^4.3.2"
	}
}